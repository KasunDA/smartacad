{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.21",
        "laravel/tinker": "1.0.2",
        "laravelcollective/html": "^5.5.1",
        "hashids/hashids": "2.0.4",
        "intervention/image": "2.4.1",
        "league/flysystem-aws-s3-v3": "1.0.18",
        "guzzlehttp/guzzle": "6.3.0",
        "baum/baum": "1.1.1",
        "barryvdh/laravel-dompdf": "0.8.1",
        "zizaco/entrust": "1.9.1",
        "graham-campbell/flysystem": "4.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.1.0",
        "filp/whoops": "2.1.12",
        "fzaninotto/faker": "1.7.1",
        "laravel/dusk": "2.0.7",
        "mockery/mockery": "1.0",
        "phpunit/phpunit": "6.4.4",
        "symfony/css-selector": "3.3.12",
        "symfony/dom-crawler": "3.3.12"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
