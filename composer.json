{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.12",
        "fideloper/proxy": "4.0.0",
        "laravel/tinker": "1.0.2",
        "laravelcollective/html": "5.6.3",
        "hashids/hashids": "2.0",
        "intervention/image": "2.4.1",
        "league/flysystem-aws-s3-v3": "1.0.18",
        "guzzlehttp/guzzle": "^6.3.0",
        "baum/baum": "1.1.1",
        "barryvdh/laravel-dompdf": "0.8.2",
        "zizaco/entrust": "1.9.1",
        "graham-campbell/flysystem": "5.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.1.4",
        "filp/whoops": "2.1.14",
        "fzaninotto/faker": "1.7.1",
        "laravel/dusk": "3.0.4",
        "mockery/mockery": "1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "7.0.2",
        "symfony/css-selector": "4.0.6",
        "symfony/dom-crawler": "4.0.6"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
