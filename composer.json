{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.4.36",
        "laravel/tinker": "~1.0.2",
        "laravelcollective/html": "^5.4.9",
        "hashids/hashids": "2.0.4",
        "intervention/image": "2.4.1",
        "league/flysystem-aws-s3-v3": "1.0.18",
        "guzzlehttp/guzzle": "6.3.0",
        "baum/baum": "1.1.1",
        "barryvdh/laravel-dompdf": "0.8.0",
        "zizaco/entrust": "1.8.0",
        "graham-campbell/flysystem": "4.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.7.1",
        "laravel/dusk": "1.*",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "6.4.4",
        "symfony/css-selector": "3.3.11",
        "symfony/dom-crawler": "3.3.11"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
